{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "import/no-extraneous-dependencies": 1,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id", "_", "_doc"]
      }
    ],
    "prefer-template": 0,
    "no-useless-escape": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "eol-last": 0,
    "indent": 0,
    "arrow-body-style": 0,
    "pr": 0,
    "comma-dangle": 0,
    "import/no-dynamic-require": 0,
    "class-methods-use-this": 0,
    "global-require": 0,
    "max-len": 0,
    "no-param-reassign": 0
  }
}